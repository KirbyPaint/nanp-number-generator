{
  "name": "nanp-number-generator",
  "version": "0.0.7",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "description": "A tool to generate only numbers that comply with the North American Numbering Plan",
  "repository": "https://github.com/KirbyPaint/nanp-number-generator.git",
  "author": "Ash Porter <ashley.porter@eyecuelab.com>",
  "keywords": [
    "phone",
    "number",
    "nanp"
  ],
  "license": "BSD-3-Clause",
  "private": false,
  "scripts": {
    "dev": "esbuild example/client.tsx --outfile=example/web/client.js --servedir=example/web --serve=6144 --bundle --minify --sourcemap",
    "build:example": "esbuild example/client.tsx --outfile=example/web/client.js --bundle --minify --sourcemap",
    "predeploy": "yarn build:example",
    "deploy": "gh-pages -d example/web",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:once": "vitest run",
    "lint": "eslint \"{src,test}/**/*.ts{,x}\"",
    "lint:fix": "yarn lint -- --fix",
    "release": "yarn build && changeset publish"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "c8": "7.13.0",
    "esbuild": "0.17.17",
    "eslint": "8.39.0",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "jsdom": "21.1.1",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "^29.1.0",
    "tsconfig-paths": "4.2.0",
    "tsup": "6.7.0",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.30.1"
  },
  "lint-staged": {
    "*.ts{,x}": "yarn lint"
  },
  "homepage": "https://github.com/KirbyPaint/nanp-number-generator#readme",
  "bugs": {
    "url": "https://github.com/KirbyPaint/nanp-number-generator/issues"
  }
}